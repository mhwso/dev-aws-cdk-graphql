{
  "name": "dev-aws-cdk-graphql",
  "version": "0.1.0",
  "bin": {
    "dev-aws-cdk-graphql": "bin/dev-aws-cdk-graphql.js"
  },
  "scripts": {
    "build": "tsc",
    "build::frontend": "cd frontend && ng build",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy::cdk": "cdk deploy --profile=cdk -O ./cdk-export.json",
    "deploy": "npm run build && npm run build::frontend && npm run deploy::cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "aws-cdk": "2.103.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.103.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
